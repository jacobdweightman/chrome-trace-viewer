{
  "name": "chrome-trace-viewer",
  "displayName": "Chrome Trace Viewer",
  "description": "View Chrome-compatible profiles and traces in VS Code!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:chrome-trace-viewer.openInChrome"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chrome-trace-viewer.openInChrome",
        "title": "Open as Profile Trace in Browser"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == json",
          "command": "chrome-trace-viewer.openInChrome"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/http-server": "^0.12.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "http-server": "^14.1.0"
  }
}
